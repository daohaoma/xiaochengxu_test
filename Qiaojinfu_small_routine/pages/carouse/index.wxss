  height: 850rpx;
}
.circle-list-item {
  margin: 20rpx 50rpx;
  background-color: #e4e4e4;
  border-radius: 4rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
}
.circle-list-item-img {
  width: 250rpx;
  height: 180rpx;
  line-height: 180rpx;
}
.circle-list-item-image {
  width: 100%;
  height: 50rpx;
}
.circle-list-item-content {
  padding: 10rpx 15rpx;
  width: 100%;
}
.item-con-part1 {
  display: flex;
  flex-direction: row;
}
.item-con-part1-status {
  width: 70rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #FCB699;
  font-size: 22rpx;
  text-align: center;
  margin-right: 10rpx;
  border-radius: 5rpx;
}
.item-con-part1-name {
  font-size: 28rpx;
}

.item-con-part2 {
  display: flex;
  justify-content: space-between;
  margin-top: 50rpx;
  font-size: 20rpx;
}
.item-con-part2-price {
  color: #CC0000;
  font-size: 20rpx;
  float: right;
}